{
  "name": "stitch-cli",
  "version": "0.0.10",
  "author": "Aviv Rozenboim @AvivRubys",
  "repository": "Soluto/stitch",
  "homepage": "https://github.com/Soluto/stitch",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "stitch": "./bin/run"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "build": "tsc",
    "test": "jest --forceExit --detectOpenHandles --config jest.config.js",
    "version": "oclif-dev readme && git add README.md"
  },
  "bugs": "https://github.com/Soluto/stitch/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "globby": "^11.0.1",
    "graphql-request": "^1.8.2",
    "js-yaml": "^3.14.0",
    "tslib": "^2.0.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.23.0",
    "@oclif/test": "^1.2.7",
    "@types/js-yaml": "^3.12.5",
    "@types/nock": "^11.1.0",
    "@types/node": "^14.14.7",
    "chai": "^4.2.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "stitch",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
